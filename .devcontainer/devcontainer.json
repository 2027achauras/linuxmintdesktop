{
	"name": "Container",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"USERNAME": "vscode",
			"BUILDKIT_INLINE_CACHE": "0"
		}
	},
	"features": {
		"ghcr.io/devcontainers/features/desktop-lite:1": {},
		"ghcr.io/devcontainers/features/docker-in-docker:1": {}
	},
	"forwardPorts": [
		6080
	],
	"portsAttributes": {
		"6080": {
			"label": "desktop"
		}
	}
}
